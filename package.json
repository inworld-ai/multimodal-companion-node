{
  "name": "multimodal-companion",
  "version": "1.0.0",
  "description": "A template project for building applications with Inworld AI",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Inworld AI",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec \"ts-node src/index.ts\" --ext ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "inworld",
    "ai",
    "template"
  ],
  "dependencies": {
    "@inworld/runtime": "https://assets.inworld.ai/node-packages/inworld-runtime-0.8.0-rc.15.tgz",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.12.0",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "multer": "^2.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "wav-encoder": "^1.3.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "typescript-eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
